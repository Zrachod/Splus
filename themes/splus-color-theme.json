	"tokenColors": [
		{
			"scope": "comment",
			"settings": {
				"foreground": "#54575A"
			}
		},
		{
			"scope": "string.quoted.double,string.quoted.single",
			"settings": {
				"foreground": "#FFED87"
			}
		},
		{
			"scope": "string.quoted.double.json,punctuation.definition.string.begin.json,punctuation.definition.string.end.json,punctuation.definition.string.begin.markdown,punctuation.definition.string.end.markdown",
			"settings": {
				"foreground": "#EEE"
			}
		},
		{
			"scope": "constant.numeric,constant.language,constant.character, constant.other",
			"settings": {
				"foreground": "#BA94FF"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#F95980"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#F9A459"
			}
		},
		{
			"scope": "storage.type,support.type.property-name",
			"settings": {
				"foreground": "#83C2EF"
			}
		},
		{
			"scope": "entity.name.type, entity.name.class, entity.other.inherited-class,entity.other.attribute-name,entity.name.function",
			"settings": {
				"foreground": "#49C9A4"
			}
		},
		{
			"scope": "support.class.js,support.function.mysql.php,support.function.js,support.type.vendored.property-name.css,support.function.gradient.css,support.function.string.php,support.constant.math.js,support.function.math.js",
			"settings": {
				"foreground": "#83C2EF"
			}
		},
		{
			"scope": "variable.parameter.url.css,punctuation.definition.string",
			"settings": {
				"foreground": "#FFED87"
			}
		},
		{
			"scope": "entity.name.tag,punctuation.definition.tag,punctuation.section.embedded",
			"settings": {
				"foreground": "#F95980"
			}
		},
		{
			"scope": "variable.parameter.js,variable.language.this.js,variable.language.arguments.js",
			"settings": {
				"foreground": "#F9A459"
			}
		},
		{
			"scope": "support.constant.property-value.css,support.function.misc.css,support.function.url.css",
			"settings": {
				"foreground": "#ff94ef"
			}
		}
	]
